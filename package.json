{
    "name": "@dintero/discounts-web-sdk",
    "version": "0.0.0-development",
    "description": "Dintero Discounts SDK for web frontends",
    "main": "dist/dintero-discounts-web-sdk.cjs.js",
    "module": "dist/dintero-discounts-web-sdk.esm.js",
    "umd:main": "dist/dintero-discounts-web-sdk.umd.min.js",
    "unpkg": "dist/dintero-discounts-web-sdk.umd.min.js",
    "types": "dist/dintero-discounts-web-sdk.cjs.d.ts",
    "preconstruct": {
        "umdName": "discounts"
    },
    "scripts": {
        "test": "jest --maxWorkers=1 --detectOpenHandles",
        "watch": "preconstruct watch",
        "build": "preconstruct build",
        "prepublishOnly": "npm run build",
        "semantic-release": "semantic-release"
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dintero/Dintero.Discounts.Web.SDK.git"
    },
    "homepage": "https://github.com/Dintero/Dintero.Discounts.Web.SDK#readme",
    "author": "Sven Nicolai Viig <sven@dintero.com> (https://dintero.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dintero/Dintero.Discounts.Web.SDK/issues"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-typescript": "^7.13.0",
        "@preconstruct/cli": "^2.1.0",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "^29.4.0",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "prettier": "^2.7.0",
        "semantic-release": "^20.1.0",
        "ts-jest": "^29.0.5",
        "typescript": "^4.2.4"
    },
    "jest": {
        "preset": "ts-jest",
        "testMatch": [
            "**/test/**/*.test.ts"
        ],
        "testEnvironment": "jsdom"
    }
}
